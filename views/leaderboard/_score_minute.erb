<div class="leaderboard-venice-rowlist">
	<div class="leaderboard-venice-rowlist-left">
		<div style="text-decoration: none;" id="Mp_Main_Score" href="#">
			<div class="leaderboard-venice-rowlist-icon">
				<img src="http://battlelog-cdn.battlefield.com/public/leaderboard/icons/Mp_Main_ScorePerMinute.png?v=3173239">
			</div>
			<div class="leaderboard-venice-rowlist-left-info ">
				<div class="leaderboard-venice-rowlist-left-info-title">SCORE PER MINUTE</div>
				<div class="leaderboard-venice-rowlist-left-info-scoretype">SPM</div>
			</div>
		</div>
	</div>						 		
	<div class="leaderboard-venice-rowlist-middle ">
		<%= partial :"leaderboard/soldier", collection: @score_minute, 
			locals: {
				:score_formater => lambda {|soldier|
					soldier.score_minute
				} 
			} %>
	</div>
</div>
